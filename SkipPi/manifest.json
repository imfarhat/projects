{
  "manifest_version": 3,
  "name": "SkipPi",
  "version": "0.0.1",
  "description": "Auto Skip YouTube Ads in less than 0.1 second (ads which are skippable after 5 seconds gets auto skipped in less than 0.1 second).",
  "author": "imfarhat.dev@gmail.com",
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },

  "action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "index.html",
    "default_title": "SkipPi"
  }
}
